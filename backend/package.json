{
    "name": "backend",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:tdd": "jest --watch --coverage",
        "test:unit": "jest --testPathPattern=src/__tests__/unit",
        "test:integration": "jest --testPathPattern=src/__tests__/integration",
        "test:e2e": "jest --testPathPattern=src/__tests__/e2e",
        "prisma:init": "npx prisma init",
        "prisma:generate": "npx prisma generate",
        "prisma:migrate": "npx prisma migrate dev",
        "prisma:studio": "npx prisma studio",
        "db:init": "ts-node src/scripts/initDb.ts",
        "db:seed": "ts-node src/scripts/seed.ts",
        "db:reset": "npx prisma migrate reset --force",
        "start:prod": "npm run build && npm start"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.13.0",
        "@types/axios": "^0.9.36",
        "@types/multer": "^1.4.12",
        "axios": "^1.9.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "multer": "^2.0.1",
        "pg": "^8.16.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.9",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.10",
        "@types/supertest": "^6.0.2",
        "eslint": "^9.2.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "prisma": "^5.13.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.9.5"
    }
}
